{
    "name": "city-of-helsinki/drupal-repository",
    "description": "Repository for City of Helsinki custom modules",
    "homepage": "https://city-of-helsinki.github.io/drupal-repository",
    "include-filename": "all.json",
    "repositories": [
        {
            "packagist.org": false
        },
        {
            "type": "vcs",
            "url": "git@github.com:druidfi/api_tools.git"
        },
        {
          "type": "vcs",
          "url": "git@github.com:City-of-Helsinki/drupal-helfi-platform.git"
        },
        {
          "type": "vcs",
          "url": "git@github.com:City-of-Helsinki/drupal-hdbt.git"
        },
        {
          "type": "vcs",
          "url": "git@github.com:City-of-Helsinki/drupal-hdbt-admin.git"
        },
        {
          "type": "vcs",
          "url": "git@github.com:City-of-Helsinki/drupal-module-helfi-api-base.git"
        },
        {
          "type": "vcs",
          "url": "git@github.com:City-of-Helsinki/drupal-module-helfi-tpr.git"
        },
        {
          "type": "vcs",
          "url": "git@github.com:City-of-Helsinki/drupal-module-helfi-tunnistamo.git"
        }
    ],
    "require": {
      "city-of-helsinki/drupal-helfi-platform": "dev-main",
      "drupal/api_tools": "8.x-1.x-dev",
      "drupal/helfi_tpr": "1.x-dev",
      "drupal/helfi_tpr": "1.x-dev",
      "drupal/helfi_api_base": "1.x-dev",
      "drupal/helfi_tunnistamo": "1.x-dev",
      "drupal/hdbt": "1.x-dev",
      "drupal/hdbt_admin": "1.x-dev"
    }
}
